{
  "name": "@bearer/core",
  "version": "0.114.1",
  "description": "Bearer Core",
  "main": "lib/main.js",
  "types": "lib/index.d.ts",
  "module": "lib/main.es.js",
  "author": "Bearer Team<engineering@bearer.sh>",
  "repository": "Bearer/bearer",
  "homepage": "https://github.com/Bearer/bearer/packages/core#readme",
  "license": "MIT",
  "files": [
    "lib/"
  ],
  "scripts": {
    "clean": "rm -rf lib/",
    "start": "yarn build -w",
    "build": "rollup -c",
    "prepack": "yarn clean && BUILD=distribution NODE_ENV=production yarn build",
    "prepare": "yarn clean && yarn build",
    "test": "jest --colors",
    "test:ci": "jest --coverage --runInBand --colors"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/preprocessor.js"
    },
    "testMatch": [
      "**/__tests__/*.+(ts|tsx|js)"
    ],
    "setupFiles": [
      "./jest.setup.js"
    ]
  },
  "devDependencies": {
    "@bearer/js": "^0.114.1",
    "@types/fbemitter": "^2.0.32",
    "@types/jest": "^24.0.11",
    "@types/lodash.get": "^4.4.6",
    "@types/lodash.merge": "^4.6.6",
    "@types/lodash.template": "^4.4.6",
    "@types/node": "10.12.18",
    "dotenv": "^6.2.0",
    "jest": "^24.7.1",
    "jest-fetch-mock": "^2.1.2",
    "per-env": "^1.0.2",
    "rollup": "^1.4.1",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-copy": "^0.2.3",
    "rollup-plugin-env": "^0.21.4",
    "rollup-plugin-filesize": "^6.0.1",
    "rollup-plugin-html-entry": "^0.2.0",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-strip": "^1.2.0",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-typescript2": "^0.19.3",
    "rollup-watch": "^4.3.1",
    "ts-jest": "^24.0.1",
    "tslint": "^5.15.0",
    "typescript": "^3.4.1"
  },
  "dependencies": {
    "@bearer/logger": "^0.114.1",
    "@bearer/types": "^0.114.1",
    "@stencil/core": "0.11.2",
    "fbemitter": "^2.1.1",
    "lodash.get": "^4.4.2",
    "lodash.merge": "^4.6.1",
    "lodash.template": "^4.4.0",
    "post-robot": "^8.0.26"
  },
  "peerDependencies": {
    "@bearer/js": ">=0.96.1"
  },
