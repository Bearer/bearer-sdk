{
  "name": "@bearer/ui",
  "version": "0.101.0",
  "description": "Bearer UI helper",
  "main": "lib/index.js",
  "module": "lib/esm/index.js",
  "types": "lib/types/components.d.ts",
  "collection": "lib/collection/collection-manifest.json",
  "repository": "Bearer/bearer",
  "homepage": "https://github.com/Bearer/bearer/packages/ui#readme",
  "author": "Bearer Team<engineering@bearer.sh>",
  "license": "MIT",
  "files": [
    "lib/"
  ],
  "scripts": {
    "clean": "rm -rf lib/",
    "build": "stencil build",
    "dev": "stencil build --dev --watch --serve",
    "serve": "stencil-dev-server",
    "start": "concurrently 'npm:dev --no-open' 'npm:storybook'",
    "test": "jest --no-cache",
    "test.watch": "jest --watch --no-cache",
    "prepare": "yarn build",
    "prepack": "BUILD=distribution yarn build",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@bearer/core": "^0.101.0",
    "@bearer/logger": "^0.101.0",
    "@stencil/core": "0.11.2",
    "@stencil/sass": "^0.1.0",
    "bootstrap": "^4.1.0",
    "rollup-plugin-strip": "^1.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@stencil/dev-server": "latest",
    "@stencil/postcss": "^0.1.0",
    "@stencil/utils": "latest",
    "@storybook/addon-actions": "^4.1.11",
    "@storybook/addon-console": "^1.1.0",
    "@storybook/html": "^4.1.11",
    "@storybook/react": "^4.1.11",
    "@types/autoprefixer": "^9.1.1",
    "@types/jest": "^23.3.13",
    "@types/node": "^10.12.26",
    "autoprefixer": "^9.3.1",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.5",
    "concurrently": "^4.1.0",
    "http-proxy-middleware": "0.19.1",
    "jest": "^24.1.0",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "rollup-plugin-replace": "^2.1.0",
    "storybook-addon-jsx": "^6.0.0"
  },
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/../../node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  },
  "gitHead": "cb2654b19000e2ad6f31a0e4ed715a6298c8df2d"
}
